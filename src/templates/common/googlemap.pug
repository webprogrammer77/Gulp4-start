#map
  script.
    var map;
    function initMap() {
    map = new google.maps.Map(document.getElementById('map'), {
    center: {
    lat: 55.72,
    lng: 37.69
    },
    scrollwheel: false,
    styles: styleArray,
    disableDefaultUI: true,
    zoom: 13
    });
    var myMarker = new google.maps.Marker({
    position: {
      lat: 55.7147,
      lng: 37.7477
    }
    , map: map, icon: '../img/icons/map_marker.svg',
    scaledSize: [50, 50]
    });
    }
    var styleArray = [{
    featureType: 'water',
    stylers: [{
    color: '#00bfa5'
    }]
    },
    {
    featureType: 'landscape',
    elementType: 'geometry.fill',
    stylers: [{
    color: '#ffffff'
    }]
    },
    {
    featureType: 'landscape.man_made',
    elementType: 'all',
    stylers: [{
    saturation: '-70'
    }]
    },
    {
    featureType: 'landscape.natural',
    elementType: 'all',
    stylers: [{
    visibility: 'off'
    }]
    },
    {
    featureType: 'poi',
    elementType: 'labels',
    stylers: [{
    visibility: 'off'
    }]
    },
    {
    featureType: 'poi.park',
    elementType: 'all',
    stylers: [{
    visibility: 'off'
    }]
    },
    {
    featureType: 'road',  
    elementType: 'all',
    stylers: [{
    lightness: '-15'
    }]
    },
    {
    featureType: 'transit',
    elementType: 'labels',
    stylers: [{
    visibility: 'off'
    }]
    }
    ];

